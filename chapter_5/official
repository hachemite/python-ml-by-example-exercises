n","  df_new['movie_avg_5'] = df_new['return_1'].rolling(window=5).mean().shift(1)\n","  df_new['movie_avg_30'] = df_new['return_1'].rolling(window=21).mean().shift(1)\n","  df_new['movie_avg_365'] = df_new['return_1'].rolling(window=252).mean().shift(1)\n","\n","\n","\n","def generate_features(df):\n","  \"\"\"\n","  Generate features for a stock/index based on historical price and\n","  performance\n","  @param df: dataframe with columns \"Open\", \"Close\", \"High\", \"Low\",\"Volume\", \"Adj Close\"\n","  @return: dataframe, data set with new features\n"," \"\"\"\n","  df_new = pd.DataFrame()\n","  #6 original features\n","  add_original_feature(df, df_new)\n","  #31 generated features\n","  add_avg_price(df, df_new)\n","  add_avg_volume(df, df_new)\n","  add_std_price(df, df_new)\n","  add_std_volume(df, df_new)\n","  add_return_feature(df, df_new)\n","  #the target\n","  df_new['close'] = df['Close']\n","  df_new = df_new.dropna()\n","  return df_new\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"vgMpKTUU3CG4"},"outputs":[],"source":["import pandas as pd\n","import numpy as np\n","\n","def generate_stock_features(df):\n","    \"\"\"\n","    Generate comprehensive technical analysis features from OHLCV stock data.\n","\n","    Parameters:\n","    -----------\n","    df : pandas.DataFrame\n","        Stock data with columns: ['Open', 'Close', 'High', 'Low', 'Volume']\n","\n","    Returns:\n","    --------\n","    pandas.DataFrame\n","        Feature matrix with 37 engineered features + target variable\n","\n","    Features Generated:\n","    - 6 original/lagged features\n","    - 18 moving averages (price + volume) with ratios\n","    - 18 volatility measures (price + volume) with ratios\n","    - 10 return/momentum indicators\n","    \"\"\"\n","\n","    # Validate input\n","    required_cols = ['Open', 'Close', 'High', 'Low', 'Volume']\n","    if not all(col in df.columns for col in required_cols):\n","        raise ValueError(f\"DataFrame must contain columns: {required_cols}\")\n","\n","    # Initialize result dataframe\n","    result = pd.DataFrame(index=df.index)\n","\n","    # Define time windows (trading days)\n","    windows = {'5': 5, '30': 21, '365': 252}  # 5d, ~1month, ~1year\n","\n","    # === ORIGINAL FEATURES (6) ===\n","    result['open'] = df['Open']\n","    for col in ['Open', 'Close', 'High', 'Low', 'Volume']:\n","        result[f'{col.lower()}_1'] = df[col].shift(1)\n","\n","    # === PRICE MOVING AVERAGES (9) ===\n","    price_avgs = {}\n","    for period, window in windows.items():\n","        col_name = f'avg_price_{period}'\n","        price_avgs[period] = df['Close'].rolling(window=window).mean().shift(1)\n","        result[col_name] = price_avgs[period]\n","\n","    # Price average ratios\n","    result['ratio_avg_price_5_30'] = price_avgs['5'] / price_avgs['30']\n","    result['ratio_avg_price_5_365'] = price_avgs['5'] / price_avgs['365']\n","    result['ratio_avg_price_30_365'] = price_avgs['30'] / price_avgs['365']\n","\n","    # === VOLUME MOVING AVERAGES (9) ===\n","    volume_avgs = {}\n","    for period, window in windows.items():\n","        col_name = f'avg_volume_{period}'\n","        volume_avgs[period] = df['Volume'].rolling(window=window).mean().shift(1)\n","        result[col_name] = volume_avgs[period]\n","\n","    # Volume average ratios\n","    result['ratio_avg_volume_5_30'] = volume_avgs['5'] / volume_avgs['30']\n","    result['ratio_avg_volume_5_365'] = volume_avgs['5'] / volume_avgs['365']\n","    result['ratio_avg_volume_30_365'] = volume_avgs['30'] / volume_avgs['365']\n","\n","    # === PRICE VOLATILITY (9) ===\n","    price_stds = {}\n","    for period, window in windows.items():\n","        col_name = f'std_price_{period}'\n","        price_stds[period] = df['Close'].rolling(window=window).std().shift(1)\n","        result[col_name] = price_stds[period]\n","\n","    # Price volatility ratios\n","    result['ratio_std_price_5_30'] = price_stds['5'] / price_stds['30']\n","    result['ratio_std_price_5_365'] = price_stds['5'] / price_stds['365']\n","    result['ratio_std_price_30_365'] = price_stds['30'] / price_stds['365']\n","\n","    # === VOLUME VOLATILITY (9) ===\n","    volume_stds = {}\n","    for period, window in windows.items():\n","        col_name = f'std_volume_{period}'\n","        volume_stds[period] = df['Volume'].rolling(window=window).std().shift(1)\n","        result[col_name] = volume_stds[period]\n","\n","    # Volume volatility ratios\n","    result['ratio_std_volume_5_30'] = volume_stds['5'] / volume_stds['30']\n","    result['ratio_std_volume_5_365'] = volume_stds['5'] / volume_stds['365']\n","    result['ratio_std_volume_30_365'] = volume_stds['30'] / volume_stds['365']\n","\n","    # === RETURNS & MOMENTUM (10) ===\n","    # Period returns\n","    return_periods = {'1': 1, '5': 5, '30': 21, '365': 252}\n","    for period, lag in return_periods.items():\n","        result[f'return_{period}'] = (\n","            (df['Close'] - df['Close'].shift(lag)) / df['Close'].shift(lag)\n","        ).shift(1)\n","\n","    # Moving average returns (momentum indicators)\n","    daily_returns = result['return_1']\n","    for period, window in windows.items():\n","        result[f'moving_avg_{period}'] = daily_returns.rolling(window=window).mean().shift(1)\n","\n","    # === TARGET VARIABLE ===\n","    result['close'] = df['Close']\n","\n","    # Remove rows with NaN values (due to rolling windows and shifts)\n","    result = result.dropna()\n","\n","    return result\n","\n","# Example usage:\n","if __name__ == \"__main__\":\n","    # Sample data structure\n","    sample_data = pd.DataFrame({\n","        'Open': [100, 101, 102, 103, 104],\n","        'High': [105, 106, 107, 108, 109],\n","        'Low': [99, 100, 101, 102, 103],\n","        'Close': [102, 103, 104, 105, 106],\n","        'Volume': [1000, 1100, 1200, 1300, 1400]\n","    })\n","\n","    # Generate features\n","    features = generate_stock_features(sample_data)\n","    print(f\"Generated {len(features.columns)} features from stock data\")\n","    print(features.columns.tolist())"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"BRZhRusCqim1"},"outputs":[],"source":["mydata = generate_features(mydata)\n","mydata"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"Jyyg7jeX2Jsa"},"outputs":[],"source":["print(\"Downloading NASDAQ Composite data (1990-2023)...\")\n","data = yf.download(\n","        \"^IXIC\",\n","        start=\"1990-01-01\",\n","        end=\"2023-06-30\",\n","        progress=False\n","    )\n","\n","data.to_csv('19900101_20230630.csv')"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"jcHLHu0Uc2pi"},"outputs":[],"source":["# Read the downloaded data from the CSV file\n","# Using header=None, index_col=0, and skiprows=2 based on previous data loading fixes\n","data_raw = pd.read_csv('19900101_20230630.csv', header=None, index_col=0, skiprows=2)\n","\n","\n","# Assign column names to the data columns after setting the index\n","data_raw.columns = ['Close', 'High', 'Low', 'Open', 'Volume']\n","\n","# The first row after skipping headers is the actual first data entry, no need for iloc[1:]\n","\n","# Set the index name for clarity\n","data_raw.index.name = 'Date'\n","\n","# Generate features using the cleaned data\n","data = generate_features(data_raw)\n","\n","print(data.round(decimals=3).head(5))"]},{"cell_type":"markdown","metadata":{"id":"1imjMD7V5lR3"},"source":["# Estimating with linear regression"]},{"cell_type":"markdown","metadata":{"id":"-vqP50_H8b1r"},"source":["## Math"]},{"cell_type":"markdown","metadata":{"id":"QESqWL5A6ouJ"},"source":[" the mean squared error (MSE):\n","\n","\n","![image.png](data:image/png;base64,)\n","\n","\n","The loss function J(w) :\n","\n","\n","  ![image.png](data:image/png;base64,)\n","\n","\n","the prediction:\n","\n","  ![image.png](data:image/png;base64,)\n","\n","\n","\n","\n"," gradient descent:\n","\n","\n"," ![image.png](data:image/png;base64,)\n","\n","\n","\n"," the gradient and learning rate 𝜂𝜂 , the weight vector w:\n","\n","\n"," ![image.png](data:image/png;base64,)\n","\n","\n","\n","\n","After a substantial number of iterations, the learned w is then used to predict a new sample x’, as follows:\n","\n","\n","\n","\n"," ![image.png](data:image/png;base64,)\n"]},{"cell_type":"markdown","metadata":{"id":"o793nRwT8di6"},"source":["## Implementing linear regression from scratch"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"7KOJl8-GVr7y"},"outputs":[],"source":["import numpy as np\n","\n","class LinearRegressionGD:\n","    def __init__(self, fit_intercept=True):\n","        self.fit_intercept = fit_intercept\n","        self.weights = None\n","\n","    def _add_intercept(self, X):\n","        \"\"\"Add intercept column if needed\"\"\"\n","        if self.fit_intercept:\n","            if self.weights is None: # First call in fit\n","                return np.hstack([np.ones((X.shape[0], 1)), X])\n","            elif X.shape[1] == len(self.weights) - 1: # Subsequent calls (including predict on new data)\n","                 return np.hstack([np.ones((X.shape[0], 1)), X])\n","        return X\n","\n","\n","    def predict(self, X):\n","        \"\"\"Make predictions using current weights\"\"\"\n","        X = self._add_intercept(X)\n","        return np.dot(X, self.weights)\n","\n","    def _compute_gradient(self, X, y):\n","        \"\"\"Compute gradient of MSE loss\"\"\"\n","        predictions = self.predict(X)\n","        error = predictions - y\n","        return np.dot(X.T, error) / len(y)\n","\n","    def _compute_loss(self, X, y):\n","        \"\"\"Compute MSE loss\"\"\"\n","        predictions = self.predict(X)\n","        return np.mean((predictions - y) ** 2) / 2\n","\n","    def fit(self, X, y, learning_rate=0.01, max_iter=1000, verbose=False):\n","        \"\"\"\n","        Train model using gradient descent\n","\n","        Parameters:\n","        - X: Feature matrix (n_samples, n_features)\n","        - y: Target values (n_samples,)\n","        - learning_rate: Step size for weight updates\n","        - max_iter: Maximum number of iterations\n","        - verbose: Whether to print progress\n","        \"\"\"\n","        X = self._add_intercept(X)\n","        self.weights = np.zeros(X.shape[1])\n","\n","        for i in range(max_iter):\n","            gradient = self._compute_gradient(X, y)\n","            self.weights -= learning_rate * gradient\n","\n","            if verbose and i % 500 == 0:\n","                loss = self._compute_loss(X, y)\n","                print(f\"Iteration {i}: Loss = {loss:.4f}\")"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"vQmcJYGXbqT4"},"outputs":[],"source":["import numpy as np\n","import matplotlib.pyplot as plt\n","\n","# Your data\n","X_train = np.array([[6], [2], [3], [4], [1], [5], [2], [6], [4], [7]])\n","y_train = np.array([5.5, 1.6, 2.2, 3.7, 0.8, 5.2, 1.5, 5.3, 4.4, 6.8])\n","\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"F6TpTs07b1R0"},"outputs":[],"source":["# Initialize and train the model\n","model = LinearRegressionGD(fit_intercept=True)\n","model.fit(X_train, y_train, learning_rate=0.01, max_iter=100, verbose=True)\n","\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"yoO4qK7Lb_fV"},"outputs":[],"source":["# Test data\n","X_test = np.array([[1.3], [3.5], [5.2], [2.8]])\n","predictions = model.predict(X_test)\n","\n","# Visualization\n","plt.figure(figsize=(10, 6))\n","\n","# Plot training data\n","plt.scatter(X_train[:, 0], y_train, marker='o', c='b', label='Training Data')\n","\n","# Plot predictions\n","plt.scatter(X_test[:, 0], predictions, marker='*', c='k', s=200, label='Predictions')\n","\n","# Plot regression line\n","x_line = np.linspace(0, 8, 100).reshape(-1, 1)\n","y_line = model.predict(x_line)\n","plt.plot(x_line, y_line, 'r-', label='Regression Line')\n","\n","plt.xlabel('x')\n","plt.ylabel('y')\n","plt.title('Linear Regression with Gradient Descent')\n","plt.legend()\n","plt.grid(True)\n","plt.show()\n","\n","# Print learned weights and predictions\n","print(\"\\nLearned weights (intercept, slope):\", model.weights)\n","print(\"Test predictions:\", predictions)"]},{"cell_type":"markdown","metadata":{"id":"uQHiav4XmFKM"},"source":["Let’s try it on another dataset, the diabetes dataset from scikit-learn:"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"IdoKg-xhmGi1"},"outputs":[],"source":["from sklearn import datasets\n","diabetes = datasets.load_diabetes()\n","print(diabetes.data.shape)\n","num_test  =30\n","X_train = diabetes.data[:-num_test,:]\n","y_train = diabetes.target[:-num_test]\n","X_train.shape\n","y_train.shape"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"xtDIel4mmV1Z"},"outputs":[],"source":["import numpy as np\n","from sklearn.datasets import load_diabetes\n","from sklearn.model_selection import train_test_split\n","\n","# Load diabetes dataset\n","diabetes = load_diabetes()\n","X = diabetes.data\n","y = diabetes.target\n","\n","# Split into train/test (last 30 samples as test)\n","num_test = 30\n","X_train, X_test = X[:-num_test], X[-num_test:]\n","y_train, y_test = y[:-num_test], y[-num_test:]\n","\n","# Initialize and train model\n","model = LinearRegressionGD(fit_intercept=True)\n","model.fit(X_train, y_train, learning_rate=1, max_iter=5000, verbose=True)\n","\n","# Make predictions\n","predictions = model.predict(X_test)\n","\n","# Print results\n","print(\"\\nPredictions vs Actual:\")\n","for pred, actual in zip(predictions, y_test):\n","      print(f\"{pred:.2f} (pred) | {actual} (actual)\")\n","\n","# Calculate MSE\n","mse = np.mean((predictions - y_test) ** 2)\n","print(f\"\\nMean Squared Error: {mse:.2f}\")\n","\n","# Plot results\n","import matplotlib.pyplot as plt\n","plt.figure(figsize=(10, 6))\n","plt.scatter(y_test, predictions, alpha=0.7)\n","plt.plot([min(y_test), max(y_test)], [min(y_test), max(y_test)], 'r--')\n","plt.xlabel('Actual Values')\n","plt.ylabel('Predicted Values')\n","plt.title('Actual vs Predicted Values')\n","plt.grid(True)\n","plt.show()\n","\n"]},{"cell_type":"markdown","metadata":{"id":"JysTukXgqr95"},"source":["# Implementing linear regression with scikit-learn"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"44C4L5dequiD"},"outputs":[],"source":["from sklearn.linear_model import SGDRegressor\n","\n","regressor = SGDRegressor(\n","    loss= \"squared_error\",\n","    penalty =  \"l2\",\n","    alpha = 0.0001,\n","    learning_rate = \"constant\",\n","    eta0 = 0.2,\n","    max_iter = 100,\n","    random_state= 42\n",")\n","\n","regressor.fit(X_train, y_train)\n","\n","predictions = regressor.predict(X_test)\n","\n","# Print results\n","print(\"\\nPredictions vs Actual:\")\n","for pred, actual in zip(predictions, y_test):\n","      print(f\"{pred:.2f} (pred) | {actual} (actual)\")\n","\n","# Calculate MSE\n","mse = np.mean((predictions - y_test) ** 2)\n","print(f\"\\nMean Squared Error: {mse:.2f}\")\n","\n","# Plot results\n","import matplotlib.pyplot as plt\n","plt.figure(figsize=(12, 6))\n","plt.scatter(y_test, predictions, alpha=0.6, label='Predictions')\n","plt.plot([min(y_test), max(y_test)], [min(y_test), max(y_test)], 'r--', label='Perfect Prediction')\n","\n","# Add residual lines\n","for actual, pred in zip(y_test, predictions):\n","    plt.plot([actual, actual], [actual, pred], 'gray', alpha=0.3)\n","\n","plt.xlabel('Actual Values')\n","plt.ylabel('Predicted Values')\n","plt.title('Actual vs Predicted Values with Residual Lines')\n","plt.legend()\n","plt.grid(True)\n","plt.show()\n","\n","plt.figure(figsize=(14, 6))\n","index = np.arange(len(y_test))\n","bar_width = 0.35\n","\n","plt.bar(index, y_test, bar_width, label='Actual', color='b', alpha=0.7)\n","plt.bar(index + bar_width, predictions, bar_width, label='Predicted', color='r', alpha=0.7)\n","\n","plt.xlabel('Sample Index')\n","plt.ylabel(